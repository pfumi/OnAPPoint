<h2>GraphTest</h2>
<div class="alert alert-@ViewBag.AlertType">
  <span class="glyphicon @ViewBag.AlertIcon" aria-hidden="true"></span>
  @ViewBag.AlertMsg
</div>
<form>
  <h3>GET</h3>
  <p>
    <button formaction="@Url.Content("~/GraphApi/ListContacts")">Contacts</button>
    <button formaction="@Url.Content("~/GraphApi/ListCalendars")">Calendars</button>
    <button formaction="@Url.Content("~/GraphApi/ListEvents")">Calendar Events</button>
    <button formaction="@Url.Content("~/GraphApi/ListMessages")">Messages</button>
  </p>
  <h3>POST</h3>
  <p></p>
  <h3>PATCH</h3>
  <p></p>
  <h3>PUT</h3>
  <p></p>
  <h3>DELETE</h3>
  <p></p>
  <div>
    @{
      if (@ViewBag.Result != null)
      {
        <hr />
        if (@ViewBag.Result.Count == 0)
        {
          <p>Empty Result..</p>
        }
        for (int i = 0; i < ViewBag.Result.Count; i++)
        {
          <h4>@ViewBag.ItemName @(i + 1)</h4>
          <p>@ViewBag.Result[i]</p>
          <hr />
        }
      }
    }
  </div>
</form>